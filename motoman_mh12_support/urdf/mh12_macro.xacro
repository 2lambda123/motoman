<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
<xacro:macro name="motoman_mh12" params="prefix">

    <!-- Properties -->
    <material name="yaskawa_blue">
          <color rgba="0.12941 0.14902 0.74902 1" />
    </material>

    <!-- link list -->
    <link name="${prefix}base_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_mh12_support/meshes/visual/MH12_BASE.STL" />
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_mh12_support/meshes/collision/MH12_BASE.STL" />
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_s">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_mh12_support/meshes/visual/MH12_S_AXIS.STL" />
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_mh12_support/meshes/collision/MH12_S_AXIS.STL" />
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_l">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_mh12_support/meshes/visual/MH12_L_AXIS.STL" />
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_mh12_support/meshes/collision/MH12_L_AXIS.STL" />
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_u">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_mh12_support/meshes/visual/MH12_U_AXIS.STL" />
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_mh12_support/meshes/collision/MH12_U_AXIS.STL" />
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_r">
        <visual>
            <origin xyz="-0.640 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_mh12_support/meshes/visual/MH12_R_AXIS.STL" />
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="-0.640 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_mh12_support/meshes/collision/MH12_R_AXIS.STL" />
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_b">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_mh12_support/meshes/visual/MH12_B_AXIS.STL"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
        	<mesh filename="package://motoman_mh12_support/meshes/collision/MH12_B_AXIS.STL" />
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_t">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_mh12_support/meshes/visual/MH12_T_AXIS.STL" />
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_mh12_support/meshes/collision/MH12_T_AXIS.STL" />
            </geometry>
        </collision>
    </link>
    <link name="${prefix}tool0"/>
    <!-- end of link list -->

    <!-- joint list -->
    <joint name="${prefix}joint_s" type="revolute">
        <parent link="${prefix}base_link"/>
        <child link="${prefix}link_s"/>
        <origin xyz="0 0 0.450" rpy="0 0 0" />
        <axis xyz="0 0 1" />
    <limit  lower="-2.9670" upper="2.9670" effort="648.27" velocity="3.8397" />
    </joint>
    <joint name="${prefix}joint_l" type="revolute">
        <parent link="${prefix}link_s"/>
        <child link="${prefix}link_l"/>
        <origin xyz="0.155 0 0" rpy="0 0 0" />
        <axis xyz="0 1 0" />
        <limit lower="-1.5708" upper="2.7052" effort="874.65" velocity="3.4906"/>
    </joint>
    <joint name="${prefix}joint_u" type="revolute">
        <parent link="${prefix}link_l"/>
        <child link="${prefix}link_u"/>
        <origin xyz="0 0 0.614" rpy="0 0 0" />
        <axis xyz="0 -1 0" />
        <limit lower="-1.4835" upper="2.6179" effort="257.25" velocity="3.8397"/>
    </joint>
    <joint name="${prefix}joint_r" type="revolute">
        <parent link="${prefix}link_u"/>
        <child link="${prefix}link_r"/>
        <origin xyz="0.640 0 0.200" rpy="0 0 0" />
        <axis xyz="-1 0 0" />
        <limit lower="-3.1415" upper="3.1415" effort="74.97" velocity="7.1558"/>
    </joint>
    <joint name="${prefix}joint_b" type="revolute">
        <parent link="${prefix}link_r"/>
        <child link="${prefix}link_b"/>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <axis xyz="0 -1 0" />
        <limit lower="-2.3562" upper="2.3562" effort="44.69" velocity="7.1558"/>
    </joint>
    <joint name="${prefix}joint_t" type="revolute">
        <parent link="${prefix}link_b"/>
        <child link="${prefix}link_t"/>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <axis xyz="-1 0 0" />
        <limit lower="-6.2832" upper="6.2832" effort="14.80" velocity="10.6465"/>
    </joint>
    <joint name="${prefix}joint_t-tool0" type="fixed">
        <origin xyz="0.100 0 0" rpy="3.1415926 -1.570796 0"/>
        <parent link="${prefix}link_t"/>
        <child link="${prefix}tool0"/>
    </joint>
    <!-- end of joint list -->

    <!-- ROS base_link to Robot Manufacturer World Coordinates transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
        <origin xyz="0 0 0.450" rpy="0 0 0"/>
        <parent link="${prefix}base_link"/>
        <child link="${prefix}base"/>
    </joint>

    </xacro:macro>
</robot>

