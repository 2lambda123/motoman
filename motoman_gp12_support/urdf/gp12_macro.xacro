<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
<xacro:macro name="motoman_gp12" params="prefix">

    <!-- Properties -->
    <material name="yaskawa_blue">
          <color rgba="0.12941 0.14902 0.74902 1" />
    </material>

    <!-- link list -->
    <link name="${prefix}base_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp12_support/meshes/visual/GP12_BASE.STL" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp12_support/meshes/collision/GP12_BASE.STL" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_s">
        <visual>
            <origin xyz="0 0 -0.450" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp12_support/meshes/visual/GP12_S_AXIS.STL" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="0 0 -0.450" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp12_support/meshes/collision/GP12_S_AXIS.STL" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_l">
        <visual>
            <origin xyz="-0.155 0 -0.450" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp12_support/meshes/visual/GP12_L_AXIS.STL" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="-0.155 0 -0.450" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp12_support/meshes/collision/GP12_L_AXIS.STL" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_u">
        <visual>
            <origin xyz="-0.155 0 -1.064" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp12_support/meshes/visual/GP12_U_AXIS.STL" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="-0.155 0 -1.064" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp12_support/meshes/collision/GP12_U_AXIS.STL" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_r">
        <visual>
            <origin xyz="-0.795 0 -1.264" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp12_support/meshes/visual/GP12_R_AXIS.STL" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="-0.795 0 -1.264" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp12_support/meshes/collision/GP12_R_AXIS.STL" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_b">
        <visual>
            <origin xyz="-0.795 0 -1.264" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp12_support/meshes/visual/GP12_B_AXIS.STL" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="-0.795 0 -1.264" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp12_support/meshes/collision/GP12_B_AXIS.STL" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_t">
        <visual>
            <origin xyz="-0.795 0 -1.264" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp12_support/meshes/visual/GP12_T_AXIS.STL" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="-0.795 0 -1.264" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://motoman_gp12_support/meshes/collision/GP12_T_AXIS.STL" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}tool0"/>
    <!-- end of link list -->

    <!-- joint list -->
    <joint name="${prefix}joint_s" type="revolute">
        <parent link="${prefix}base_link"/>
        <child link="${prefix}link_s"/>
        <origin xyz="0 0 0.450" rpy="0 0 0" />
        <axis xyz="0 0 1" />
        <limit lower="-2.9670" upper="2.9670" effort="926.10" velocity="4.5355"/>
    </joint>
    <joint name="${prefix}joint_l" type="revolute">
        <parent link="${prefix}link_s"/>
        <child link="${prefix}link_l"/>
        <origin xyz="0.155 0 0" rpy="0 0 0" />
        <axis xyz="0 1 0" />
        <limit lower="-1.5708" upper="2.7052" effort="1029.00" velocity="4.0132"/>
    </joint>
    <joint name="${prefix}joint_u" type="revolute">
        <parent link="${prefix}link_l"/>
        <child link="${prefix}link_u"/>
        <origin xyz="0 0 0.614" rpy="0 0 0" />
        <axis xyz="0 -1 0" />
        <limit lower="-1.4835" upper="2.6179" effort="551.25" velocity="4.5375"/>
    </joint>
    <joint name="${prefix}joint_r" type="revolute">
        <parent link="${prefix}link_u"/>
        <child link="${prefix}link_r"/>
        <origin xyz="0.640 0 0.200" rpy="0 0 0" />
        <axis xyz="-1 0 0" />
        <limit lower="-3.4906" upper="3.4906" effort="89.96" velocity="8.2007"/>
    </joint>
    <joint name="${prefix}joint_b" type="revolute">
        <parent link="${prefix}link_r"/>
        <child link="${prefix}link_b"/>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <axis xyz="0 -1 0" />
        <limit lower="-2.6179" upper="2.6179" effort="67.03" velocity="8.1992"/>
    </joint>
    <joint name="${prefix}joint_t" type="revolute">
        <parent link="${prefix}link_b"/>
        <child link="${prefix}link_t"/>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <axis xyz="-1 0 0" />
        <limit lower="-7.9412" upper="7.9412" effort="33.30" velocity="12.2143"/>
    </joint>
    <joint name="${prefix}joint_t-tool0" type="fixed">
        <origin xyz="0.100 0 0" rpy="3.1415926 -1.570796 0"/>
        <parent link="${prefix}link_t"/>
        <child link="${prefix}tool0"/>
    </joint>
    <!-- end of joint list -->

    <!-- ROS base_link to Robot Manufacturer World Coordinates transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
        <origin xyz="0 0 0.450" rpy="0 0 0"/>
        <parent link="${prefix}base_link"/>
        <child link="${prefix}base"/>
    </joint>

    </xacro:macro>
</robot>

