<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
<xacro:macro name="motoman_gp8" params="prefix">

    <!-- Properties -->
    <material name="yaskawa_blue">
          <color rgba="0.12941 0.14902 0.74902 1" />
    </material>

    <!-- link list -->
    <link name="${prefix}base_link">
        <visual>
            <origin xyz="0 0 0" rpy="1.570796 0 3.1415926" />
            <geometry>
                <mesh filename="package://motoman_gp8_support/meshes/visual/GP8_BASE.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="1.570796 0 3.1415926" />
            <geometry>
                <mesh filename="package://motoman_gp8_support/meshes/collision/GP8_BASE.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_s">
        <visual>
            <origin xyz="0 0 -0.330" rpy="1.570796 0 3.1415926" />
            <geometry>
                <mesh filename="package://motoman_gp8_support/meshes/visual/GP8_S_AXIS.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="0 0 -0.330" rpy="1.570796 0 3.1415926" />
            <geometry>
                <mesh filename="package://motoman_gp8_support/meshes/collision/GP8_S_AXIS.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_l">
        <visual>
            <origin xyz="-0.040 0 -0.330" rpy="1.570796 0 3.1415926" />
            <geometry>
                <mesh filename="package://motoman_gp8_support/meshes/visual/GP8_L_AXIS.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="-0.040 0 -0.330" rpy="1.570796 0 3.1415926" />
            <geometry>
                <mesh filename="package://motoman_gp8_support/meshes/collision/GP8_L_AXIS.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_u">
        <visual>
            <origin xyz="-0.040 0 -0.675" rpy="1.570796 0 3.1415926" />
            <geometry>
                <mesh filename="package://motoman_gp8_support/meshes/visual/GP8_U_AXIS.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="-0.040 0 -0.675" rpy="1.570796 0 3.1415926" />
            <geometry>
                <mesh filename="package://motoman_gp8_support/meshes/collision/GP8_U_AXIS.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_r">
        <visual>
            <origin xyz="-0.380 0 -0.715" rpy="1.570796 0 3.1415926" />
            <geometry>
                <mesh filename="package://motoman_gp8_support/meshes/visual/GP8_R_AXIS.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="-0.380 0 -0.715" rpy="1.570796 0 3.1415926" />
            <geometry>
                <mesh filename="package://motoman_gp8_support/meshes/collision/GP8_R_AXIS.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_b">
        <visual>
            <origin xyz="-0.380 0 -0.715" rpy="1.570796 0 3.1415926" />
            <geometry>
                <mesh filename="package://motoman_gp8_support/meshes/visual/GP8_B_AXIS.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="-0.380 0 -0.715" rpy="1.570796 0 3.1415926"/>
            <geometry>
                <mesh filename="package://motoman_gp8_support/meshes/collision/GP8_B_AXIS.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_t">
        <visual>
            <origin xyz="-0.380 0 -0.715" rpy="1.570796 0 3.1415926" />
            <geometry>
                <mesh filename="package://motoman_gp8_support/meshes/visual/GP8_T_AXIS.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <origin xyz="-0.380 0 -0.715" rpy="1.570796 0 3.1415926" />
            <geometry>
                <mesh filename="package://motoman_gp8_support/meshes/collision/GP8_T_AXIS.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}tool0"/>
    <!-- end of link list -->

    <!-- joint list -->
    <joint name="${prefix}joint_s" type="revolute">
        <parent link="${prefix}base_link"/>
        <child link="${prefix}link_s"/>
        <origin xyz="0 0 0.330" rpy="0 0 0" />
        <axis xyz="0 0 1" />
        <limit lower="-2.9670" upper="2.9670" effort="150.92" velocity="7.9412"/>
    </joint>
    <joint name="${prefix}joint_l" type="revolute">
        <parent link="${prefix}link_s"/>
        <child link="${prefix}link_l"/>
        <origin xyz="0.040 0 0" rpy="0 0 0" />
        <axis xyz="0 1 0" />
        <limit lower="-1.1344" upper="2.5307" effort="176.4" velocity="6.7495"/>
    </joint>
    <joint name="${prefix}joint_u" type="revolute">
        <parent link="${prefix}link_l"/>
        <child link="${prefix}link_u"/>
        <origin xyz="0 0 0.345" rpy="0 0 0" />
        <axis xyz="0 -1 0" />
        <limit lower="-1.2217" upper="3.3161" effort="107.56" velocity="9.0757"/>
    </joint>
    <joint name="${prefix}joint_r" type="revolute">
        <parent link="${prefix}link_u"/>
        <child link="${prefix}link_r"/>
        <origin xyz="0.340 0 0.040" rpy="0 0 0" />
        <axis xyz="-1 0 0" />
        <limit lower="-3.3161" upper="3.3161" effort="55.86" velocity="9.5993"/>
    </joint>
    <joint name="${prefix}joint_b" type="revolute">
        <parent link="${prefix}link_r"/>
        <child link="${prefix}link_b"/>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <axis xyz="0 -1 0" />
        <limit lower="-2.3561" upper="2.3561" effort="32.68" velocity="9.5993"/>
    </joint>
    <joint name="${prefix}joint_t" type="revolute">
        <parent link="${prefix}link_b"/>
        <child link="${prefix}link_t"/>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <axis xyz="-1 0 0" />
        <limit lower="-6.2944" upper="6.2944" effort="22.54" velocity="17.4845"/>
    </joint>
    <joint name="${prefix}joint_t-tool0" type="fixed">
        <origin xyz="0.080 0 0" rpy="3.1415926 -1.570796 0"/>
        <parent link="${prefix}link_t"/>
        <child link="${prefix}tool0"/>
    </joint>
    <!-- end of joint list -->

    <!-- ROS base_link to Robot Manufacturer World Coordinates transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
        <origin xyz="0 0 0.330" rpy="0 0 0"/>
        <parent link="${prefix}base_link"/>
        <child link="${prefix}base"/>
    </joint>

    </xacro:macro>
</robot>

