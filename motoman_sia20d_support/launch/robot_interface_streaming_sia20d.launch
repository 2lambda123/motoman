<!--
  Manipulator specific version of 'robot_interface_streaming.launch'.

  Defaults provided for sia20d:
   - 7 joints

  Usage:
    robot_interface_streaming_sia20d.launch robot_ip:=<value> controller:=<fs100|dx100>
-->
<launch>
	<arg name="robot_ip" />
	
  <!-- controller: Controller name (fs100 or dx100) -->
  <arg name="controller"/>

	<rosparam command="load" file="$(find fanuc_m10ia_support)/config/joint_names_sia20d.yaml" />

	<include file="$(find motoman_driver)/launch/robot_interface_streaming_$(arg controller).launch">
		<arg name="robot_ip"   value="$(arg robot_ip)" />
	</include>
</launch>
